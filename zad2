public class Exer2 {
  String name,last_name;
  Exer2() {
    this.name = "Kamil";
    this.last_name = "Kowalski";
  }
  public void Name() {
    System.out.println("Imie: " + this.name + " " + this.last_name);
  }
  public static void main(String[] args) {
    Exer2 human = new Exer2();
    human.Name();
  }
}
